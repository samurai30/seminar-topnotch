{% extends 'base.html.twig' %}

{% block title %}Top-Notch{% endblock %}

{% block body %}
    {{ include('loader/loader.html.twig') }}

<div class="container">

     <div class="row">



         <div class="col s12 m8 offset-m2">

            <div class="center-align">
                <a id="menu" class="waves-effect waves-light btn btn-floating red lighten-3" onclick="$('.tap-target').tapTarget('close');"><i class="material-icons">menu</i></a>

                <!-- Tap Target Structure -->
                <div class="tap-target amber" data-target="menu">
                    <div class="tap-target-content">
                        <h5>Title</h5 >
                        <p>A bunch of text</p>
                    </div>
                </div>
            </div>
         </div>
    </div>


</div>
{% endblock %}
{% block javascripts %}
    {{ parent() }}

{% endblock %}